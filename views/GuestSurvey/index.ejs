<div class="homepage">
  <div class="center-frame">
    <div class="semi-transparent-background"></div>
      <h1><span id="hheader">Vacation Calculator</span></h1>
      <table id="questiontable">
        <tr>
          <td id="gc1" class="surveytext">How many people will vacation together?</td>
        </tr>
        <tr>
          <td id="gc2" class="surveytext">What will accommodations generally cost per night?</td>
        </tr>
        <tr>
          <td id="gc3" class="surveytext">How many days do you vacation in a year?</td>
        </tr>
        <tr>
          <td id="gc4" class="surveytext">How many years do you plan on vacationing?</td>
        </tr>
        <tr>
          <td id="gc5" class="surveytext">What do you think inflation will be over 10 years?</td>
        </tr>
      </table>

<div id="reviewbox">
      <div id="reviewleft" class="fadeinout">
      <table>
          <tbody>
            <tr>
              <td id="gs1" class="remixer"></td>
              <td id="survey1" class="surveytext setwidth"></td>
            </tr>
            <tr>
              <td id="gs2" class="remixer"></td>
              <td id="survey2"class="surveytext setwidth"></td>
            </tr>
            <tr>
              <td id="gs3" class="remixer"></td>
              <td id="survey3"  class="surveytext setwidth"></td>
            </tr>
            <tr>
              <td id="gs4" class="remixer"></td>
              <td id="survey4"  class="surveytext setwidth"></td>
            </tr>
            <tr>
              <td id="gs5" class="remixer"></td>
              <td id="survey5"  class="surveytext setwidth"></td>
            </tr>
          </tbody>
      </table>
    </div>

    <div id="reviewrighta" class"fadeinout">
      <div class="numberno">
        <h2>1</h2>
      </div>
      <table>
        <tr>
          <td class="basictext largetext">Inflation: </td>
          <td id="ownermaintenance" class="bluetext largetext">0%</td>
        </tr>
        <tr>
          <td class="basictext largetext">Total Duration Cost: </td>
          <td id="totalcost" class="ownermoney largetext"></td>
        </tr>
      </table>
    </div>
    <div id="reviewrightb" class"fadeinout">
      <div class="numberno">
        <h2>2</h2>
      </div>
      <table>
        <tr>
          <td class="basictext largetext">Inflation: </td>
          <td id="inflationheader" class="bluetext largetext">0%</td>
        </tr>
        <tr>
          <td class="basictext largetext">Yearly Cost: </td>
          <td id="totalcost2" class="bluetext largetext"></td>
        </tr>
        <tr>
          <td class="basictext largetext">Inflation Cost: </td>
          <td id="totaldiff" class="bluetext largetext"></td>
        </tr>
        <tr>
          <td class="basictext largetext">Total Duration Cost: </td>
          <td id="totalinflation" class="ownermoney largetext"></td>
        </tr>
      </table>
    </div>
</div>

  <%- partial('../Shared/_display_buttons.ejs') %>

  </div>
</div>

<% block('localScripts', '<script src="/page_js/star-explosion.js"></script>') %>
<% block('localScripts', '<script src="/page_js/fireworks.js"></script>') %>
<% block('localScripts', '<script src="/page_js/survey.js"></script>') %>
